{
  "title": "JishoSeibiki",
  "name": "jishoseibiki",
  "version": "1.1.0",
  "description": "Japanese dictionary lookup from the context menu.",
  "main": "index.js",
  "author": "Nathan Henninger",
  "homepage": "https://github.com/nhenninger",
  "icon": "",
  "engines": {
    "firefox": ">=38.0a1",
    "fennec": ">=38.0a1"
  },
  "license": "MIT",
  "permissions": {
    "cross-domain-content": ["https://glosbe.com/", "http://nihongo.monash.edu/", "http://jisho.org"],
    "private-browsing": true
  },
  "preferences": [
    {
      "name": "dictPref",
      "type": "radio",
      "title": "Dictionary",
      "value": "JDIC",
      "options": [
        {
          "value": "WWWJDIC",
          "label": "Jim Breen's WWWJDIC"
        },
        {
          "value": "GLOSBE_COM",
          "label": "Glosbe.com"
        },
        {
          "value": "JISHO_ORG",
          "label": "Jisho.org"
        }
      ]
    }
  ]
}
